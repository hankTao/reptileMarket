.tabBar {
    display: flex;
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 98rpx;
    background: #fff;
    z-index: 11;
    justify-content: space-between;
    align-items: center;
    box-shadow: 6rpx 6rpx 6rpx 6rpx #ddd;
}

.tabBar-item {
    color: #999;
    text-align: center;
}

.tabBar-item view:nth-child(2) {
    font-size: 20rpx;
}

.tabBartext {
    color: #ffb62b;
}

.tabBar-item:nth-child(1) {
    margin-left: 30rpx;
}

.tabBar-item:last-child {
    margin-right: 30rpx;
}

.tabBar-item:nth-child(3) view:nth-child(1) {
    position: relative;
}

.tabBar-item:nth-child(3) view:nth-child(1) image {
    position: absolute;
    left: -10rpx;
    top: -60rpx;
    width: 60rpx;
    height: 60rpx;
}

.tabBar-item:nth-child(3) view:nth-child(2) {
    margin-top: 52rpx;
}